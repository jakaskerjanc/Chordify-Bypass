{
  "name": "Chordify Bypass",
  "description": "Bypass chord view limit",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://chordify.net/*"],
  "permissions": ["cookies"]
}
